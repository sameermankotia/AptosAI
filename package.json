{
    "name": "aptos-ai",
    "version": "1.0.0",
    "description": "AI Framework for Aptos blockchain - Smart contract interaction and analysis using AI",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
      "build": "tsc",
      "start": "ts-node examples/demo.ts",
      "dev": "next dev",
      "test": "jest",
      "lint": "eslint . --ext .ts,.tsx",
      "format": "prettier --write \"src/**/*.{ts,tsx}\"",
      "prepare": "husky install",
      "demo": "ts-node examples/demo.ts",
      "build:docs": "typedoc --out docs src/",
      "clean": "rimraf dist",
      "prebuild": "npm run clean",
      "prepack": "npm run build"
    },
    "dependencies": {
      "@radix-ui/react-alert-dialog": "^1.0.5",
      "@radix-ui/react-select": "^2.0.0",
      "@radix-ui/react-tabs": "^1.0.4",
      "aptos": "^1.20.0",
      "class-variance-authority": "^0.7.0",
      "clsx": "^2.0.0",
      "dotenv": "^16.3.1",
      "lucide-react": "^0.263.1",
      "next": "^14.0.0",
      "openai": "^4.0.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "recharts": "^2.10.3",
      "tailwind-merge": "^2.0.0",
      "tailwindcss-animate": "^1.0.7"
    },
    "devDependencies": {
      "@types/jest": "^29.5.11",
      "@types/node": "^20.10.5",
      "@types/react": "^18.2.45",
      "@types/react-dom": "^18.2.18",
      "@typescript-eslint/eslint-plugin": "^6.15.0",
      "@typescript-eslint/parser": "^6.15.0",
      "autoprefixer": "^10.4.16",
      "eslint": "^8.56.0",
      "eslint-config-next": "14.0.4",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-react": "^7.33.2",
      "husky": "^8.0.3",
      "jest": "^29.7.0",
      "postcss": "^8.4.32",
      "prettier": "^3.1.1",
      "rimraf": "^5.0.5",
      "tailwindcss": "^3.4.0",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.2",
      "typedoc": "^0.25.4",
      "typescript": "^5.3.3"
    },
    "peerDependencies": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "engines": {
      "node": ">=16.0.0"
    },
    "keywords": [
      "aptos",
      "blockchain",
      "ai",
      "defi",
      "smart-contracts",
      "web3",
      "cryptocurrency",
      "trading",
      "analysis"
    ],
    "author": "Sameer Mankotia",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/yourusername/aptos-ai.git"
    },
    "bugs": {
      "url": "https://github.com/yourusername/aptos-ai/issues"
    },
    "homepage": "https://github.com/yourusername/aptos-ai#readme",
    "files": [
      "dist",
      "src",
      "LICENSE",
      "README.md"
    ],
    "husky": {
      "hooks": {
        "pre-commit": "npm run lint && npm run test"
      }
    },
    "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "testMatch": [
        "**/__tests__/**/*.ts",
        "**/?(*.)+(spec|test).ts"
      ]
    }
  }